<Window x:Class="ExtractSlidoContent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExtractSlidoContent"
        mc:Ignorable="d"
        Title="MainWindow" Height="480" Width="800"
        WindowStartupLocation="CenterOwner">
    <Grid>
        <Button Content="Upload" Name="Upload" HorizontalAlignment="Left" Margin="666,50,0,0" VerticalAlignment="Top" Width="107" Height="28" Click="Upload_Click"/>
        <TextBox IsEnabled="False" x:Name="fileContent" HorizontalAlignment="Left" Margin="10,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="651" Height="28"/>
        <Button Content="Get Questions" Name="Questions" HorizontalAlignment="Left" Margin="666,388,0,0" Width="107" Height="28" VerticalAlignment="Top" Click="Questions_Click"/>
        <Label Name="questionsCount" HorizontalAlignment="Left" Margin="24,388,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
